#include <iostream>

using namespace std;

bool fulg(int n)
{
    if(int(log10(n)+1==9))
    {
        for(int i=0;i<strlen(n);i++)
        {
            if(n[i]==1 or n[i]==2 or n[i]==3 or n[i]==4);
            else return false;
        }
        return true;
    }
    else return false;
}
int main()
{
    cin>>n;
    cout<<fulg(n);
    return 0;
}
